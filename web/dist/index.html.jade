  script.
    function set_active_submenu_item() {
      $('.submenu li').removeClass('active');
      $('.submenu li').each(function (index, li) {
        var href = $(li).find('a').attr('href')
        if (window.location.href.match(new RegExp(href + '$'))) {
          $(li).addClass('active');
        }
      });
    }


    $(document).on('click', set_active_submenu_item);
    $(document).on('ready', set_active_submenu_item);

