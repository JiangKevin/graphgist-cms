    div
      div.marquee.blue.center
        div.row.text-center
          div.medium-10.small-centered.columns
            h1.entry-title GraphGists

      div
        div(ng-controller="GistSearchForm")
          ul.submenu
            li
              a(href="/#!/gists")
                h5 Featured GraphGists
            li
              a(href="#!/gists/about")
                h5 What is a GraphGist?
            li
              a(href="/#!/gists/all")
                h5 All GraphGists
            li
              a(href="#!/gists/submit")
                h5 Submit a GraphGist
            li.has-form
              form#searchform.searchform.ng-pristine.ng-valid(role="search", method="get", action="http://neo4j.com/", ng-class="{searching: query.length > 0}")
                input#s(type="text", name="s", ng-model="query", placeholder="Search for a GraphGist")
                input#searchsubmit(type="image", src="http://neo4j.com/wp-content/themes/neo4jweb/assets/images/searchicon.png")
              #search-result(ng-show="query.length > 0", ng-cloak)
                div(ng-if="results")
                  .list-group
                    a.list-group-item(href="#!/gists/{{result.id}}/summary", ng-click="search_result_clicked()", ng-repeat="result in results")
                      .thumbnail.pull-right
                        img(src="{{result.poster_image}}")
                      {{result.title}}
                div(ng-if="!results || !results.length")
                  .search-notice
                    span(ng-if="!results") Searching...
                    span(ng-if="results && !results.length") No results found




        div(ng-view)

            

